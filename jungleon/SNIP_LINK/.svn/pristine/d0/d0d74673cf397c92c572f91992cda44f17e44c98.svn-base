.b_bottom_layer {width:95%;padding:9px 8px 9px 8px;border:1px solid #d8d8d8;background:#ffffff;position:absolute;overflow:hidden}
.b_bottom_layer h3 {width:120px;margin-bottom:10px;font-weight:bold;font-size:1em;color:#202020;float:left}
.b_bottom_layer span {float:right}
.b_bottom_layer .layer_table {width:100%;border-top:3px solid #5b5b5b;border-bottom:2px solid #5b5b5b}
.b_bottom_layer .layer_table tbody th {padding:9px 0;color:#3d3d3d;background:#f4f5f7;border-width:1px;border-style:none none solid none;border-color:#dbdbdb}
.b_bottom_layer .layer_table tbody td {padding:9px 0;text-align:center;color:#3d3d3d;border-width:1px;border-style:none none solid none;border-color:#dbdbdb}



.tab {width:100%;margin-bottom:10px;border-bottom:2px solid #adadad;overflow:hidden}
.tab li {margin-right:5px;float:left;text-align:center}
.tab li a {width:100px;padding:10px 0;display:inline-block;color:#000000;background:#ebebeb;font-weight:bold;font-size:1.1em}
.tab li a:hover {color:#ffffff;background:#303030}
.tab li a.on {color:#ffffff;background:#303030}



.config .b_bottom {padding:9px 15px 9px 15px;position:relative; border:1px solid #d8d8d8;width:45%}
.config .b_bottom h3 {padding:9px 15px 9px 15px!important;margin-bottom:10px!important;font-weight:bold!important;font-size:1em!important;color:#202020!important}

table.people_num {width:100%;border-top:3px solid #5b5b5b;border-bottom:2px solid #5b5b5b}
table.people_num thead th {padding:9px 0;color:#3d3d3d;background:#f4f5f7;border-width:1px;border-style:none solid solid none;border-color:#dbdbdb}
table.people_num thead th.tb_right {border-width:1px;border-style:none none solid none;border-color:#dbdbdb}
table.people_num tbody td {padding:9px 0;text-align:center;color:#3d3d3d;border-width:1px;border-style:none solid solid none;border-color:#dbdbdb}
table.people_num tbody td.tb_right {border-width:1px;border-style:none none solid none;border-color:#dbdbdb}
table.people_num tbody td.b_last {border-width:1px 1px 2px 1px;border-style:none solid solid none;border-color:#dbdbdb #dbdbdb #5b5b5b #dbdbdb}
table.people_num tbody td.b_last_right {border-width:1px 1px 2px 1px;border-style:none none solid none;border-color:#dbdbdb #dbdbdb #5b5b5b #dbdbdb} 
table.people_num .last {border-right:0}