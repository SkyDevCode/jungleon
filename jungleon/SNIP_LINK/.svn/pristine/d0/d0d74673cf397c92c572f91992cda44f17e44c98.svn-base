/* 달력  */
.ui-datepicker-trigger {margin-left:5px;vertical-align:middle;cursor:pointer} 

.center .content .page h3 {padding:20px 0 20px 5px;font-size:2.3em;font-weight:bold;color:#202020}
.center .content .page .location {padding-top:10px;text-align:right}
.center .content .page .location span {color:#202020} 
.center .content .page .location span img {margin-top:1px}

.search_form_top {padding:10px 0 10px 10px;margin-bottom:10px;text-align:left;position:relative;background:#ebebeb}
.search_form_top button {vertical-align:middle}
.search_form_top a {position:absolute;top:8px;right:0}
.search_form_top select {width:100px;height:20px;border:1px solid #d3d3d3}
.search_form_top input.txt_box {width:130px;height:20px;border:1px solid #d3d3d3}
.search_form_bottom {margin-top:10px;text-align:center}
.search_form_bottom select {width:100px;height:20px;border:1px solid #d3d3d3}

.table_top_btn {margin-bottom:9px;overflow:hidden}
.table_top_btn li {margin-right:5px;float:right} 

/*그래프*/
.graph_box {padding:0 0 30px 30px}
.graph_box .graph {position:relative;width:100%;background:url(../../common_adm/img/bg/back/graph_bg02.gif) repeat-x left top}
.graph_box .txt {padding-bottom:10px;text-align:right}
.graph_box .txt span {display:inline-block;*zoom:1;*display:inline;position:relative;padding-left:22px}
.graph_box .txt .y {margin-right:10px}
.graph_box .txt .y {background:url(../../common_adm/img/bg/icon/color_icon01.gif) no-repeat left 4px}
.graph_box .txt .t {background:url(../../common_adm/img/bg/icon/color_icon02.gif) no-repeat left 4px}
.graph_box .graph .g {position:relative;width:100%;height:179px;overflow:hidden;text-align:center}
.graph_box .graph .g li {display:inline-block;zoom:1;*display:inline;width:40px;margin-right:10%}
.graph_box .graph .g li div {position:absolute;bottom:0;width:20px}
.graph_box .graph .g li span {}
.graph_box .graph .num {position:absolute;left:-38px;top:-8px}
.graph_box .graph .num span {dispaly:inline-block;*zoom:1;*display:inline;width:31px;text-align:right}
.graph_box .graph .num span.n01 {position:absolute;left:0;top:2px}
.graph_box .graph .num span.n03 {position:absolute;left:0;top:163px}
.graph_box .graph .num span.n02 {position:absolute;left:0;top:82px}
.graph_box .graph .red {left:0;background:url(../../common_adm/img/common/bg_red.gif) repeat-y left bottom}
.graph_box .graph .purple {left:19px;background:url(../../common_adm/img/common/bg_blue.gif) repeat-y left bottom}
.tlt_box {width:100%;overflow:hidden}
.tlt_box h4 {float:left}
.tlt_box button {float:right}
/*그래프*/

