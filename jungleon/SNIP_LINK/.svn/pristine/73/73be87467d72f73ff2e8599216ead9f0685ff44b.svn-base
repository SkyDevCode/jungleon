	.main .content .r2 > .popupzone{width: 33.33%;height: 319px;}
	.main .content .r2 > .bbswrap{width: 66.66%;margin-left: 0;height: 319px;}
	.main .content .r2 .mtab{padding-left: 0;}
	.main .content .r2 .mtab li{width: 33.33%;margin-left: 0;height: 50px;line-height: 50px;}
	.main .content .r2 .latestBbs{height: 255px;}
