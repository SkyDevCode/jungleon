	#visual .owl-theme .owl-controls .owl-nav .owl-prev{left: 10px;}
	#visual .owl-theme .owl-controls .owl-nav .owl-next{right: 10px;}
}

@media screen and (max-width: 1320px){
	.main .content .r4 > .w .portfolio .owl-controls .owl-nav{display: none;}
}

@media screen and (max-width: 1219px) and (min-width: 1025px){
	.sliderWrap li .txt{max-width: 1024px;}
	.content .w{max-width: 1024px;}
}

