	.main .content .r2{margin-top: -1px;}
	.main .content .r2 > .popupzone{width: 100%;height: 289px;}
	.popupzone .popupcontroler{left: initial;right: 5px;top: 15px;}
	.popupzone .btn{width: 31px;height: 37px;background-size: contain !important;margin-top: -18px;}
	.main .content .r2 > .bbswrap{width: 100%;margin-top: 10px;height: 245px;}
	.main .content .r2 .latestBbs{height: 205px;}
	.main .content .r2 .latestBbs > .n_l{padding: 0;}
	.main .content .r2 .mtab li{font-size: 15px;height: 40px;line-height: 40px;}
	.main .content .r2 .mtab li.on a{background: none;padding-right: 0;display: block;}
	.main .content .r2 .latestBbs > .n_l > li{padding: 10px;}
	.main .content .r2 .latestBbs > .n_l > li .subj{font-size: 14px;}
	.main .content .r2 .latestBbs > .n_l > li .date{font-size: 14px;}
