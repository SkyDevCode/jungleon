	.main .content .r2 .latestBbs > .btn_more a span{display: none;}

	.main .content .r3{margin-top: 15px;height: 280px;}
	.main .content .r4 > .w{padding: 25px 0;}
	.main .content .r3 > .w > ul li{height: 280px;}
