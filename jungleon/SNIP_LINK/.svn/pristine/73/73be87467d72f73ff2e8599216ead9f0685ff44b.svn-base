@charset "utf-8";

/*slider*/
.sliderWrap{position:relative;}
.sliderWrap .bg li{width:100%; height:850px;position: relative;}
.sliderWrap li .txt{display: block;width: 100%;height: 100%;max-width:1200px;margin: 0 auto;box-sizing: border-box;padding-top: 300px;color:#fff;}
#visual > .sliderWrap li .txt.l{text-align: left;}
#visual > .sliderWrap li .txt.c{text-align: center;}
#visual > .sliderWrap li .txt.r{text-align: right;}
#visual > .sliderWrap li .txt.t{padding-top: 150px;}
#visual > .sliderWrap li .txt.m{padding-top: 320px;}
#visual > .sliderWrap li .txt.b{padding-top: 490px;}
.sliderWrap li .txt > h3 span{display: inline-block;font-size: 40px;padding-bottom: 10px;}
.sliderWrap li .txt > h3 img{width: auto;display: inline-block;}
.sliderWrap li .txt > h3.stitn{display: none;}
.sliderWrap li .txt > h3.stit1 span{display: none;}
.sliderWrap li .txt > h3.stit2 img{display: none;}
.sliderWrap li .txt > p span{display: inline-block;font-size: 14px;line-height: 18px;}
.sliderWrap li .txt > p img{width: 50%;display: inline-block;}
.sliderWrap li .txt > p.stxtn{display: none;}
.sliderWrap li .txt > p.stxt1 span{display: none;}
.sliderWrap li .txt > p.stxt2 img{display: none;}
.sliderWrap li .txt > span.sbtnn{display: none;}
.sliderWrap li .txt > span.sbtn1{display: inline-block;padding: 8px 60px;color: #fff;font-size: 18px;margin-top: 55px;border: 5px solid #fff;box-sizing: border-box;position: relative;z-index: 0;}
.sliderWrap li .txt > span.sbtn1:before{content: "";width: 0;height: 100%;display: block;position: absolute;left: 0;top: 0;transition: 0.5s;z-index: -1;}
.sliderWrap li .txt > span.sbtn1:hover:before{width:100%;background: linear-gradient(to right, #ff9966 0%, #ff5e62 100%);}
.sliderWrap ul.bg > li:first-child{display:block;}
.sliderWrap ul.bg > li img{position:absolute;left:50%;top:0;margin-left:-950px;max-width:auto;width:auto;max-width:inherit;}

#visual .owl-theme .owl-controls .owl-dots{position:absolute;left:50%;top:240px;
	transform: translateX(-50%);}
.owl-theme .owl-controls .owl-dots .owl-dot {display: inline-block;}
.owl-theme .owl-controls .owl-dots .owl-dot span {border-radius: 50%;display: block;width: 13px;height: 13px;margin: 0 15px;background-color: rgba(255,255,255,0.6);}
.owl-theme .owl-controls .owl-dots .owl-dot.active span {background-color:#d90011;}

#visual .owl-theme .owl-controls .owl-nav > div{position: absolute;display: inline-block !important;height: 88px;width: 88px;background: url('') 0 0/contain no-repeat;top:50%;transform:translateY(-50%);}
#visual .owl-theme .owl-controls .owl-nav .owl-prev{left: 10px;background-image: url('../img/main/visual-btn-prev.png');}
#visual .owl-theme .owl-controls .owl-nav .owl-next{right: 10px;background-image: url('../img/main/visual-btn-next.png');}

#container{position: relative;}
.w{max-width: 1200px;margin: 0 auto;}
#container.main{background-color: #eaeaea;border-bottom: 1px solid #cacaca;}
#container.main .content{position: relative;z-index: 1;padding-bottom: 45px;margin-top: -175px;}

.main .content .r1 > ul{display: table;width: 100%;border-left: 1px solid #c9c9c9;}
.main .content .r1 > ul li{display: table-cell;width: 33.33%;height: 336px;border: 1px solid #c9c9c9;box-sizing: border-box;vertical-align: middle;background-color: #fff;border-left:none;text-align: center;}
.main .content .r1 > ul li a{color: #5c5c5c;}
.main .content .r1 > ul li .t1{font-size: 22px;display: block;padding-top: 75px;}
.main .content .r1 > ul li .t2{display: block;margin-top: 10px;font-size: 15px;}
.main .content .r1 > ul li .btn{display: inline-block;width: 40px;height: 40px;border-radius: 50%;background: #d01524 url('../img/main/r1-btn-arrow.png') center no-repeat;margin-top: 25px;font-size: 0;color: #fff;line-height: 40px;}
.main .content .r1 > ul li.n1 .t1{background: url('../img/main/r1-n1-off.png') center top no-repeat;background-size: 74px 67px;}
.main .content .r1 > ul li.n2 .t1{background: url('../img/main/r1-n1-off.png') center top no-repeat;background-size: 74px 67px;}
.main .content .r1 > ul li.n3 .t1{background: url('../img/main/r1-n1-off.png') center top no-repeat;background-size: 74px 67px;}
.main .content .r1 > ul li:hover{background-color: #d01524;}
.main .content .r1 > ul li:hover a{color: #fff;}
.main .content .r1 > ul li.n1:hover .t1{background-image: url('../img/main/r1-n1-on.png');}
.main .content .r1 > ul li.n2:hover .t1{background-image: url('../img/main/r1-n1-on.png');}
.main .content .r1 > ul li.n3:hover .t1{background-image: url('../img/main/r1-n1-on.png');}
.main .content .r1 > ul li:hover .btn{width: 110px;font-size: 15px;text-align: left;background-position: right center;}

.main .content .r2{margin-top: 15px;font-size: 0;}
.main .content .r2 > .popupzone{display: inline-block;vertical-align: top;width: 400px;height: 329px;border: 1px solid #c9c9c9;box-sizing: border-box;background-color: #fff;overflow: hidden;position: relative;}
.popupzone > .popup_w{width:100%;height:100%;position: relative;}
.popupzone > .popup_w .item{position:absolute;display:none;left:0;top:0;width:100%;height:100%;overflow: hidden;}
.popupzone > .popup_w .item a{display: block;width: 100%;height: 100%;position: relative;overflow: hidden;}
.popupzone > .popup_w .item a img{position: absolute;max-width: initial;height: 100%;left: 50%;top: 50%;transform: translate(-50%, -50%);}
.popupzone > .popup_w .item:first-child{display:block;}
.popupzone .popupcontroler{position:absolute;z-index:2;left:25px;top:25px;}
.popupzone .bullit{width:12px;height:12px;margin:0 8px;background-color: #8a8a8a;border-radius: 50%;}
.popupzone .bullit.on{background-color: #d01524;}
.popupzone .btn{position: absolute;z-index: 2;top: 50%;width: 51px;height: 57px;margin-top: -25px;opacity: 0.7;}
.popupzone .btn.prev{left: 0px;background: url(../img/main/event-btn-prev.png) right center no-repeat;}
.popupzone .btn.next{right: 0px;background: url(../img/main/event-btn-next.png) left center no-repeat;}
.popupzone .btn:hover{opacity: 1;}

.main .content .r2 > .bbswrap{display: inline-block;vertical-align: top;width: 774px;margin-left: 26px;height: 329px;overflow: hidden;}
.main .content .r2 .mtab{font-size: 0;padding-left: 1px;}
.main .content .r2 .mtab li{font-size: 20px;height: 60px;line-height: 60px;display: inline-block;vertical-align: top;width: 200px;text-align: center;border: 1px solid #c9c9c9;margin-left: -1px;background-color: #fff;box-sizing: border-box;}
.main .content .r2 .mtab li a{display: inline-block;color: #5c5c5c;}
.main .content .r2 .mtab li.on{background-color: #7a7a7a;}
.main .content .r2 .mtab li.on a{color: #fff;padding-right: 20px;background: url('../img/main/bbs-tab-more.png') right 24px no-repeat;}
.main .content .r2 .mtab li.on a:hover{text-decoration: none;}

.main .content .r2 .latestBbs{height: 270px;background-color: #fff;box-sizing: border-box;border: 1px solid #c9c9c9;margin-top: -1px;}
.main .content .r2 .latestBbs > .tit{display: none;}
.main .content .r2 .latestBbs > .btn_more{display: none;}
.main .content .r2 .latestBbs > .n_l{padding: 10px 30px 20px;}
.main .content .r2 .latestBbs > .n_l > li{padding: 20px 0;border-bottom: 1px solid #e0e0e0;}
.main .content .r2 .latestBbs > .n_l > li .subj{font-size: 15px;width: 85%;display: inline-block;vertical-align: middle;text-overflow: ellipsis;white-space: nowrap;overflow: hidden;color: #5c5c5c;}
.main .content .r2 .latestBbs > .n_l > li .subj a{padding-left: 15px;background: url('../img/main/subj-dot.png') 0 8px no-repeat;text-align: left;color: #5c5c5c;}
.main .content .r2 .latestBbs > .n_l > li .date{font-size: 15px;width: 15%;text-align: right;display: inline-block;vertical-align: middle;}
.main .content .r2 .latestBbs > .n_l > li .cont{display: none;}
.main .content .r2 .latestBbs > .n_l > li .more{display: none;}

.main .content .r3{background: url('../img/main/r3-bg.jpg') center/cover no-repeat;height: 370px;margin-top: 50px;}
.main .content .r3 > .w > ul{display: table;width: 100%;}
.main .content .r3 > .w > ul li{display: table-cell;width: 25%;height: 370px;vertical-align: middle;text-align: center;}
.main .content .r3 > .w > ul li a{color: #fff;}
.main .content .r3 > .w > ul li .t1{display: block;margin-top: 20px;font-size: 20px;}
.main .content .r3 > .w > ul li .t2{display: block;margin-top: 15px;font-size: 15px;}
.main .content .r3 > .w > ul li .btn{display: block;height: 34px;background: url('../img/main/r3-btn-bg.png') center top no-repeat;margin-top: 25px;}

.main .content .r3 > .w > ul li .progress{width: 111px;height: 111px;line-height: 111px;margin: 0 auto;position: relative;border-radius: 4px;overflow: hidden;}
.main .content .r3 > .w > ul li .progress:after{content: "";width: 100%;height: 100%;border-radius: 50%;border: 2px solid #fff;position: absolute;box-sizing: border-box;width: 100%;height: 100%;top: 0px;left: 0px;}
.main .content .r3 > .w > ul li:hover .progress:after{width: 98%;height: 98%;top: 1px;left: 1px;}
.main .content .r3 > .w > ul li .progress > span{width: 50%;height: 100%;overflow: hidden;position: absolute;top: 0;z-index: 1;display: none;}
.main .content .r3 > .w > ul li:hover .progress > span{display: block;}
.main .content .r3 > .w > ul li .progress .progress-bar{width: 100%;height: 100%;transition: width 0.3s ease;border: 4px solid #d01524;position: absolute;top: 0;box-sizing: border-box;}
.main .content .r3 > .w > ul li .progress .progress-left{left: 0;}
.main .content .r3 > .w > ul li .progress .progress-left .progress-bar{left: 100%;border-top-right-radius: 80px;border-bottom-right-radius: 80px;border-left:0;transform-origin: center left;animation: loading-1 0.3s linear forwards;}
.main .content .r3 > .w > ul li .progress .progress-right{right: 0;}
.main .content .r3 > .w > ul li .progress .progress-right .progress-bar{left: -100%;border-top-left-radius: 80px;border-bottom-left-radius: 80px;border-right:0;transform-origin: center right;animation: loading-1 0.3s linear forwards 0.3s;}
.main .content .r3 > .w > ul li .progress .progress-icon img{vertical-align: middle;}
@keyframes loading-1{
		0%{
				-webkit-transform: rotate(360deg);
				transform: rotate(360deg);
		}
		100%{
				-webkit-transform: rotate(180deg);
				transform: rotate(180deg);
		}
}

.main .content .r4 > .w{padding: 45px 0 25px;}
.main .content .r4 > .w .sec-tit{font-size: 30px;color: #000;text-align: center;font-weight: 500;}
.main .content .r4 > .w .sec-tit .btn-more{display: inline-block;margin-left: 15px;}
.main .content .r4 > .w .sec-tit .btn-more a{display: inline-block;text-indent: -9999px;width: 25px;background: url('../img/main/btn-more.png') 0 center no-repeat;}
.main .content .r4 > .w .portfolio{font-size: 0;margin-top: 30px;}
.main .content .r4 > .w .portfolio.pc{display: block;}
.main .content .r4 > .w .portfolio.mobile{display: none;}
.main .content .r4 > .w .portfolio li{display: inline-block;border: 1px solid #c9c9c9;box-sizing: border-box;background-color: #fff;position: relative;width: 100%;height: 240px;overflow: hidden;z-index: 2;}
.main .content .r4 > .w .portfolio li a{display: block;width: 100%;height: 100%;position: relative;overflow: hidden;}
.main .content .r4 > .w .owl-carousel .owl-stage-outer{padding-left: 1px;}
.main .content .r4 > .w .portfolio .owl-dots{text-align: center;margin-top: 20px;}
.main .content .r4 > .w .portfolio .owl-controls .owl-dots .owl-dot span{width: 11px;height: 11px;margin: 0 8px;background-color: #bbb;}
.main .content .r4 > .w .portfolio .owl-controls .owl-dot.active span{background-color: #d01524;}
.main .content .r4 > .w .portfolio .owl-controls .owl-nav > div{position: absolute;display: inline-block;height: 33px;width: 19px;background: url('') 0 0/contain no-repeat;top:105px;}
.main .content .r4 > .w .portfolio .owl-controls .owl-nav .owl-prev{left: -49px;background-image: url('../img/main/portfolio-btn-prev.png');}
.main .content .r4 > .w .portfolio .owl-controls .owl-nav .owl-next{right: -49px;background-image: url('../img/main/portfolio-btn-next.png');}
.main .content .r4 > .w .portfolio li .btn{width: 106px;height: 0;background: linear-gradient(to right, #ff9966 0%, #ff5e62 100%);position: absolute;left: 50%;margin-left: -53px;bottom: 68px;opacity: 0;border-radius: 20px;line-height: 38px;text-align: center;color: #fff;font-size: 15px;}
.main .content .r4 > .w .portfolio li img{max-width:initial;height: 168px;position: absolute;left: 50%;top: 0;transform: translateX(-50%);transition: all 0.3s;}
.main .content .r4 > .w .portfolio li .t1{display: block;text-align: center;font-size: 18px;color: #000;height: 70px;line-height: 70px;position: absolute;left: 50%;bottom: 0;transform: translateX(-50%);width: 100%;}
.main .content .r4 > .w .portfolio li:hover img{margin-top: -15px;}
.main .content .r4 > .w .portfolio li:hover .t1{padding-top: 15px;}
.main .content .r4 > .w .portfolio li:hover .btn{height: 38px;opacity: 1;transition: 0.3s;}

.main .content .r5{height: 260px;padding: 50px 0 45px;background: url('../img/main/r5-bg.jpg') center no-repeat;box-sizing: border-box;color: #fff;text-align: center;}
.main .content .r5 .sec-tit{font-size: 37px;font-weight: bold;}
.main .content .r5 .sec-txt{font-size: 15px;margin-top: 10px;}
.main .content .r5 > ul{margin-top: 30px;font-size: 0;}
.main .content .r5 > ul > li{display: inline-block;font-size: 15px;height: 40px;line-height: 40px;width: 130px;border-radius: 25px;border: 2px solid #fff;margin: 0 25px;position: relative;transition: 0.3s;}
.main .content .r5 > ul > li a{display: block;color: #fff;}
.main .content .r5 > ul > li:hover{background-color: #fff;}
.main .content .r5 > ul > li:hover:before{content: "";display: block;width: 18px;height: 16px;background: url('../img/main/r5-btn-bg.png') 0 0 no-repeat;position: absolute;left: 50%;margin-left: -9px;top: -8px;}
.main .content .r5 > ul > li:hover a{color: #000;}

