	#visual > .sliderWrap li .txt.m{padding-top: 240px;}
	#visual .owl-theme .owl-controls .owl-dots{top: 120px;}

	.main .content .r1 > ul li{height: 246px;}
	.main .content .r1 > ul li .t2{display: none;}

