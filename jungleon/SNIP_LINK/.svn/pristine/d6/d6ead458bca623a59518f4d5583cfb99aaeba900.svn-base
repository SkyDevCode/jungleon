.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item>a.on{background-color:#fff; border-right:none; color:#005baa !important;}
.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item2>a{top:42px;}
.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item3>a{top:84px;}
.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item4>a{top:126px;}
.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item5>a{top:168px;}

.gnb_depth2_item1{display:block;}
.gnb_depth2_item{
	/*position:relative; top:0; border:none; background:none;background-color:#fff;*/
	display: none;
    position: fixed;
    top: 100px;
    left: 120px;
    z-index: 1;
    overflow: hidden;
    overflow-y: auto;
    width: calc(100% - 120px);
    height: calc(100% - 100px);
    border-left: 1px solid #c0c9cd;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    border: none;
    background: none;
    background-color: #fff;
}
.gnb_depth2_item>div{background:none;}
.gnb_depth2_inner{padding:0; }
.gnb_depth2_inner .sgnb_title_txt{display:none;}
.gnb_depth2_list>ul>li{float:none; padding:0; width:100%; min-height:10px;}
.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item{float:none; padding:0;}
.gnb_depth2_item{ padding:0 15px; }
.gnb_depth2_list>ul{border:none;}
.gnb_depth2_list>ul>li:before{display:none;}
.gnb_depth2_list>ul>li>a{padding:8px 20px 8px 5px; display:block; font-size:15px; border-bottom:1px solid #d8d8d8; }
.gnb_depth2_list>ul>li>a.gnb_dep3_open{background:url(../img/common/mobile_gnb_down.png) no-repeat right 5px center; background-size:8px;}
.gnb_depth2_list>ul>li>a.gnb_dep3_open.on{background:url(../img/common/mobile_gnb_up.png) no-repeat right 5px center; background-size:8px; color:#005baa;}
.gnb_dep3_list{padding:6px 0; border-bottom:1px solid #d8d8d8; display:none;}
.gnb_dep3_list>ul>li{margin:0; }
.gnb_dep3_list>ul>li>a{display:inline-block;padding:6px 0 6px; font-size:14px; color:#424242;}
.gnb_dep3_list>ul>li:before{top:14px;left:5px; }
.gnb_dep4_list{display:none;}
.gnb_depth2_item1 .gnb_depth2_list>ul>li,
.gnb_depth2_item2 .gnb_depth2_list>ul>li,
.gnb_depth2_item3 .gnb_depth2_list>ul>li,
.gnb_depth2_item4 .gnb_depth2_list>ul>li,
.gnb_depth2_item5 .gnb_depth2_list>ul>li{padding:0; width:100%; min-height:10px;}
.mobile_menu_close{position:absolute; right:15px; top:15px; z-index:23; width:20px; height:20px; text-indent:-9999px; background:url(../img/common/mobile_gnb_close.jpg) no-repeat center center;  background-size:20px; display:block;}
.gnb_dep3_list>ul>li>a.gnb_dep4_open{background:none; padding-right:0;}

}
.all-menu__wrap{position:fixed; left:0; top:0; bottom:0; z-index:30; width:100%; height:100%; background-color:#006ab8; overflow:auto; color:#fff; display:none;}
.all-menu__inner{position:relative; margin:0 auto; padding:0 20px; max-width:1240px; }
.all_menu_depth1{position:relative;}
.all-menu__wrap h1{margin-bottom:50px; padding:25px 0; border-bottom:1px solid #4e97cc; line-height:1; text-align:center; font-size:36px;}
.all_menu_depth1_item{float:left; padding:0 25px; width:20%; box-sizing:border-box;}
.all_menu_depth1_item .depth1{margin-bottom:45px; padding:10px; text-align:center; border:1px solid #81b5dd; font-size:25px; font-weight:400;}
.all_menu_depth2_list>ul>li{margin-bottom:35px;}
.all_menu_depth2_list>ul>li>a{font-size:22px; font-weight:700;}
.all_menu_dep3_list>ul>li{position:relative; margin-top:14px; padding-left:10px; }
.all_menu_dep3_list>ul>li>a{font-size:17px; font-weight:400; color:#dfe5f1;}
.all_menu_dep3_list>ul>li:before{position:absolute; left:0; top:8px; width:3px; height:3px; background-color:#b3d3ec; display:block; content:"";}
.all_menu_dep4_list>ul>li{position:relative; margin-top:6px; padding-left:10px;}
.all_menu_dep4_list>ul>li:before{position:absolute; left:0; top:8px; width:4px; height:1px; background-color:#b3d3ec; display:block; content:"";}
.all_menu_dep4_list>ul>li>a{font-size:15px; font-weight:400; color:#dfe5f1;}
.all_menu_depth1_item:before{position:absolute; left:0; top:0; bottom:0; width:1px; height:100%; background-color:#3389c6; display:block; content:"";}
.all_menu_depth1_item1:before{display:none;}
.all_menu_depth1_item2:before{left:20%;}
.all_menu_depth1_item3:before{left:40%;}
.all_menu_depth1_item4:before{left:60%;}
.all_menu_depth1_item5:before{left:80%;}
.total_menu_close{position:absolute; right:24px; top:28px; }

@media screen and (max-width:1240px){
.all_menu_depth1_item{padding:0 15px; }
.all_menu_depth1_item .depth1{font-size:20px;}
.all_menu_depth2_list>ul>li>a{font-size:18px; }
.all_menu_dep3_list>ul>li>a{font-size:15px;}
.all_menu_dep4_list>ul>li>a{font-size:13px;}
}
@media screen and (max-width:1024px){
.all-menu__wrap h1{margin-bottom:30px; font-size:30px; }
.all_menu_depth1_item{padding:0 5px;}
.all_menu_depth1_item .depth1{margin-bottom:20px; font-size:16px; }
.all_menu_depth2_list>ul>li{margin-bottom:15px; }
.all_menu_depth2_list>ul>li>a{font-size:16px; }
.all_menu_dep3_list>ul>li{margin-top:10px;}

}

.anchor{display:none;}
