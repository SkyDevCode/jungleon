@charset "utf-8";

html, body {height:100%}
@media screen and (max-width: 1240px) {
	.no_scroll {overflow: hidden !important; width: 100%; height: 100% !important;}
}
/* common */
.skip a{overflow:hidden;display:block;position:absolute;top:0;left:0;width:1px;height:1px;background:#000;color:#fff;text-align:center}
.skip a:focus,.skip a:active{position:relative;width:100%;height:auto;padding:10px}
.inner{max-width:1240px; margin:0 auto;}
.only_mob{display: none;}
.sub-online{margin: 0 auto; min-width:1240px; }

/*header*/
.header__wrap{position:absolute; left:0; top:0; z-index:10; width:100%; border-bottom:1px solid rgba(255,255,255,0.2);}
.header__wrap--utill{position:relative; z-index:2; border-bottom:1px solid rgba(255,255,255,0.2);}
.header__inner{padding:0 20px; margin:0 auto; max-width:1400px; }
.header__inner:after{ clear: both; content: "."; display: block; height: 0; visibility: hidden; font-size: 0;}
.header__inner .logo{float:left; padding:30px 0 18px; }
.header__inner .header__utils{float:right; padding:0 0 0 30px; }
.header__inner .header__utils:after{ clear: both; content: "."; display: block; height: 0; visibility: hidden; font-size: 0;}
.header__inner .header__utils li{float:left; position:relative; padding:0 30px; }
.header__inner .header__utils li.item-language{padding:0; }
.header__inner .header__utils li a{font-size:14px; color:#fff; }
.header__inner .header__utils li button{padding:5px 12px; border:1px solid #fff; display:inline-block; font-size:13px; color:#fff; }
.header__inner .header__utils li button:after{margin-left:5px; display:inline-block; width:7px; height:4px; background:url(../img/common/web_language_down_wh.png) no-repeat center center; display:inline-block; content:""; }
.main_header__wrap .header__inner .header__utils li button:after{background:url(../img/common/web_language_down_wh.png) no-repeat center center; }
.main_header__wrap .header__inner .header__utils li button.on:after{background:url(../img/common/web_language_wh.png) no-repeat center center;}
.header__inner .header__utils li button.on:after{background:url(../img/common/web_language_wh.png) no-repeat center center;}
.header__inner .header__utils li .item-language_list{position:absolute; left:0; top:28px; z-index:4; width:100%; background-color:#fff; border:1px solid #e5e5e5; font-size:13px; display:none;}
.header__inner .header__utils li .item-language_list li{padding:0; float:none; width:100%; margin-bottom:0; border-top:1px solid #e5e5e5; text-align:center; }
.header__inner .header__utils li .item-language_list li a{padding:2px 0; display:block; color:#565656;}
.header__inner .header__utils li .item-language_list li:first-child{border-top:none;}
.header__inner .header__utils li.item-link a{margin-top:-2px; width:102px; height:26px; line-height:26px; background-color:#01a0e2; font-size:13px; color:#fff; display:inline-block; text-align:center; }
.header__inner .header__utils li:last-child{padding-right:0;}
.header__inner .search-form_wrap{position:absolute; left:50%; top:16px; margin-left:-216px; width:432px; height:55px;}
.header__inner .search-form_wrap .search-form_inner{position:relative; }
.header__inner .search-form_wrap .search-form_inner input[type=search]{padding:0 45px 0 10px; width:100%; height:55px; line-height:37px; box-sizing:border-box; background:rgba(0,0,0,0.3); border:2px solid rgba(127,190,37,0.7);}
.header__inner .search-form_wrap .search-form_inner .btn-search{position:absolute; right:0; top:0; min-width:50px; width:55px; height:55px; background:url(../img/common/btn_search_wh.png) no-repeat center center; text-indent:-9999px; overflow:hidden;}
.header__inner .search-form_wrap .search-form_inner input[type=search]::placeholder {color:#fff; font-size:15px;  font-weight: 400;}
.header__wrap--bottom{float:left; padding:45px 0 0 60px ; }
.header__wrap--bottom .header__wrap--inbox{margin:0 auto; box-sizing:border-box; text-align:right;}
.header__wrap--bottom .header__wrap--inbox .gnb_depth1{float:left; }
.header__wrap--bottom .header__wrap--inbox .gnb_depth1:after{ clear: both; content: "."; display: block; height: 0; visibility: hidden; font-size: 0;}
.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item{float:left; padding:0 60px;}
.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item>a{position:relative; padding:0 5px 20px; display:block; font-size:24px; color:#fff; font-family:"Roboto"; font-weight:400;}
.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item.gnb_over>a:after{position:absolute; left:50%; bottom:-9px; z-index:3; margin-left:-11px; width:21px; height:9px; background:url(../img/common/gnb_arrow.png) no-repeat center 0; display:block; content:"";}
.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item.on>a{position:relative; }
.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item.on>a:after{position:absolute; left:0; bottom:0; width:100%; height:1px; background-color:#ffa024; display:block; content:"";}

.main_header__wrap .open_all_menu{background-color:#f47320; }
.main_header__wrap .open_all_menu_wrap{top:95px;}
.main_header__wrap .open_all_menu button span,
.main_header__wrap .open_all_menu button span:before,
.main_header__wrap .open_all_menu button span:after{background-color:#ffa024; }

.open_all_menu_wrap{position:absolute; right:0; top:87px; z-index:2;  display:block;}
.open_all_menu{float:right; padding-top:14px; width:78px; height:60px; box-sizing:border-box; }
.open_all_menu button{position:relative; margin:0 auto; width:26px; display:block; height:34px; box-sizing:border-box;  text-indent:-9999px; }
.open_all_menu button span{position:absolute; left:0; top:6px; width:100%; height:3px; background-color:#fff; display:inline-block; vertical-align:top;}
.open_all_menu button span:before{position:absolute; left:0; top:9px; width:18px; height:3px; background-color:#fff; display:block; content:"";-webkit-transition: all 0.35s ease-out;-moz-transition: all 0.35s ease-out;-ms-transition: all 0.35s ease-out;-o-transition: all 0.35s ease-out;transition: all 0.35s ease-out;}
.open_all_menu button span:after{position:absolute; left:0; top:18px; width:100%; height:3px; background-color:#fff; display:block; content:"";}
.open_all_menu button:hover span:before{width:100%;}
.mobile_menu_open_wrap{display:none;}
.gnb_util{display:none;}
.mobile_menu_close{display:none;}

.header__wrap.header_fixed{position:fixed; background-color:#fff; box-shadow:5px 5px 3px rgba(100,100,100,0.2);}
.header__wrap.header_fixed .header__wrap--utill{display:none;}
.header__wrap.header_fixed .header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item>a{color:#363636;}
.header__wrap.header_fixed .open_all_menu a span,
.header__wrap.header_fixed .open_all_menu a span:before,
.header__wrap.header_fixed .open_all_menu a span:after{background-color:#333; }
.header__wrap.header_fixed .header__inner .header__utils li button,
.header__wrap.header_fixed .header__inner .header__utils li a{color:#333;}
.header__wrap.header_fixed .header__wrap--bottom{padding:30px 0;}


@media screen and (max-width:1460px){
.header__wrap--bottom .header__wrap--inbox .gnb_depth1>.gnb_depth1_item{padding:0 30px; }
}

