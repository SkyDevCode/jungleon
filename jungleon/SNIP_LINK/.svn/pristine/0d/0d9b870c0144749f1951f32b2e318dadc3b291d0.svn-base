/* style03*/
.tb03 {width:100%;border-style:solid none none;border-width:3px 0 0;border-color:#5b5b5b}
.tb03 thead th {padding:8px 10px 8px 10px;border-style:none solid solid none;border-width:0 1px 1px 0;border-color:#adadad;background-color:#fafbfd;color:#3d3d3d}
.tb03 tbody th {padding:10px;border-style:none solid solid none;border-width:0 1px 1px 0;border-color:#adadad;background-color:#F4F4F4;color:#3d3d3d}
.tb03 tbody td {padding:10px;border-style:none solid solid none;border-width:0 1px 1px 0;border-color:#adadad;text-align:center}
.tb03 tfoot th {padding:10px;border-style:none solid solid none;border-width:0 1px 1px 0;border-color:#adadad;background-color:#fafbfd;color:#3d3d3d}
.tb03 tfoot td {padding:10px;border-style:none solid solid none;border-width:0 1px 1px 0;border-color:#adadad;text-align:center}
.tb03 tbody td ol {padding-left:23px}
