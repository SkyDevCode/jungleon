.tb01_v tbody td a:hover {text-decoration:underline;color:#3f79b1}
.tb01_v tbody td a:focus {text-decoration:underline;color:#3f79b1}
.tb01_v tbody td input.input01 {width:173px;border:1px solid #cecece}
