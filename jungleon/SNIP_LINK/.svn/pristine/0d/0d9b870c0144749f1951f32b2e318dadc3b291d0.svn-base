.txt_view h3 {padding:9px 0 9px 20px;border-top:3px solid #5b5b5b;border-bottom:1px solid #dbdbdb;background-color:#f4f5f8;font-size:1em;font-weight:900;color:#3d3d3d}
.txt_view dl {width:100%;border-bottom:1px solid #dbdbdb;overflow:hidden}
.txt_view dl.top {border-top:3px solid #5b5b5b}
.txt_view dl.dl_b {width:100%;border-bottom:1px solid #adadad;overflow:hidden}
.txt_view dt {float:left;width:80px;padding:11px 0 11px 20px;background:url("../../resources/common/img/bg/icon/bullet01.gif") no-repeat right 50%;font-size:0.9em;font-weight:900;color:#3d3d3d;text-align:left}
.txt_view dd {float:left;width:251px;padding:11px 0 11px 20px;font-size:0.9em}
.txt_view dd a:hover {text-decoration:underline;color:#3f79b1}
.txt_view dd a:focus {text-decoration:underline;color:#3f79b1}
.txt_view div {padding:11px 0 110px 20px;border-bottom:1px solid #adadad;font-size:0.9em}
.prev_next {width:100%;overflow:hidden}
.prev_next dt {float:left;width:57px;padding:10px 0 10px 30px;border-bottom:1px solid #dbdbdb;color:#3d3d3d}
.prev_next dt.p {background:url("../../resources/common/img/bg/icon/bullet12.gif") no-repeat 15px 18px}
