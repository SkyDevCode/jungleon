.tb01 tbody td {padding:10px 0;border-style:none none solid;border-width:0 0 1px;border-color:#dbdbdb;text-align:center}
.tb01 tbody td.tb_left {padding-left:15px;text-align:left}
.tb01 tbody td a:hover {text-decoration:underline;color:#3f79b1}
