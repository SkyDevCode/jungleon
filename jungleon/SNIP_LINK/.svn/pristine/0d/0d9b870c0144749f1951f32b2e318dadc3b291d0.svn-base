.tb01_v tbody td {padding:11px 15px;border-style:none none solid;border-width:0 0 1px;border-color:#adadad;text-align:left}
.tb01_v tbody td .photo {width:100px;height:140px;margin:0 auto;border:1px solid #ababab}
