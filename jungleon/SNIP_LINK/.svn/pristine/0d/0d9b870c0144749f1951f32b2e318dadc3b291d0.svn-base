.prev_next dd a:hover {text-decoration:underline;color:#3f79b1}
.prev_next dd a:focus {text-decoration:underline;color:#3f79b1}
.tb_dl01 dt {padding-left:10px;background:url("../../resources/common/img/common/bullet07.gif") no-repeat left 8px}
.tb_dl01 dd {padding-left:10px;background:url("../../resources/common/img/common/bullet08.gif") no-repeat left 9px}
.tb_dl01 dd em {display:block;padding-left:5px;background:url("../../resources/common/img/common/bullet09.gif") no-repeat left 8px}
.td_ul01 li {padding-left:10px;background:url("../../resources/common/img/common/bullet08.gif") no-repeat left 9px}
.td_ul01 li li {padding-left:10px;background:url("../../resources/common/img/common/bullet06.gif") no-repeat left 9px}
.td_sp01 {display:inline-block;padding-left:10px;background:url("../../resources/common/img/common/bullet08.gif") no-repeat left 9px}
.file {display:inline-block;background:url("../../resources/common/img/common/icon_file.gif") no-repeat left 3px}
.tb_align01 td {vertical-align:top;text-align:left !important}
