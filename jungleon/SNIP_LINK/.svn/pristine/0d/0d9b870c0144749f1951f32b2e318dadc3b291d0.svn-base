/* paging */
.paging_box {position:relative;width:100%;margin-top:20px}
.paging_box .btn {position:absolute;right:0;top:0}
.paging {text-align:center}
.paging strong {padding:3px 8px;color:#ffffff;background-color:#5b5b5b;border:1px solid #5b5b5b}
.paging a {padding:3px 8px;color:#666666;background-color:#ffffff;border:1px solid #e0e0e0}
.paging a img {vertical-align:middle}
.paging a:hover {padding:3px 8px;color:#ffffff;background-color:#5b5b5b;border:1px solid #5b5b5b}
.paging a:focus {padding:3px 8px;color:#ffffff;background-color:#5b5b5b;border:1px solid #5b5b5b}
.paging .prev1 {padding:0 0 0 0;border:none;background:none}
